module Lec1 where

id : (x:Type) -> x -> x
id = \x y . y 

idid : ((x:Type) -> (y : x) -> x) 
idid = id ((x:Type) -> (y : x) -> x) id

compose : (A : Type) -> (B : Type) -> (C:Type) ->
  (B -> C) -> (A -> B) -> (A -> C)
compose = \ A B C f g x. (f (g x))

-- some Church encodings: booleans

bool : Type 
bool = (A:Type) -> A -> A -> A 

true : bool
true = \A x y. x

false : bool
false = \A x y. y

cond : bool -> (x:Type) -> x -> x -> x
cond = \ b . b 

void : Type
void = (x:Type) -> x

unit : Type
unit =  (x:Type) -> x -> x

-- need definitional equality for this one

bool' : bool -> Type 
bool' = \b . (B : (b : bool) -> Type) -> (B true) -> B false -> B b

true' : bool' true
true' = \A x y . x

false' : bool' false
false' = \ A x y. y



